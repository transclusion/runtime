{
  "name": "@transclusion/runtime-batch",
  "version": "1.0.0-alpha.2",
  "description": "",
  "author": "Marius Lundg√•rd <studio@mariuslundgard.com>",
  "license": "MIT",
  "main": "dist/commonjs/index.js",
  "module": "dist/module/index.js",
  "typings": "dist/module/index.d.ts",
  "scripts": {
    "build": "npm-run-all tsc-*",
    "clean": "rimraf dist",
    "format": "prettier --write \"{src,test}/**/*.{js,ts,tsx}\"",
    "lint": "tslint -c tslint.json '{src,test}/**/*.{ts,tsx}'",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "tsc-commonjs": "tsc -p .tsconfig/commonjs.json",
    "tsc-module": "tsc -p .tsconfig/module.json"
  },
  "peerDependencies": {
    "@transclusion/runtime-core": "^1.0.0-alpha.2"
  },
  "devDependencies": {
    "@transclusion/runtime-core": "^1.0.0-alpha.2",
    "@transclusion/vdom": "^1.0.0-beta.4",
    "@types/jest": "^22.2.2",
    "@types/node": "^9.6.1",
    "jest": "^22.4.3",
    "jest-cli": "^22.4.3",
    "npm-run-all": "^4.1.1",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.2",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.10.0",
    "typescript": "^2.8.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(\\.|/)(test|spec)\\.tsx?$",
    "moduleFileExtensions": [
      "js",
      "ts"
    ]
  }
}
